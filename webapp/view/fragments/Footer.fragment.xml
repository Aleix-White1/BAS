<core:FragmentDefinition
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<Toolbar id="footerButtons" class="customFooter">
		<Button icon="sap-icon://pushpin-off" press="onPressToolbarButton" class="tmbButton">
			<customData>
				<core:CustomData key="buttonTarget" value="RoutePushpin"/>
			</customData>
		</Button>
		<Button icon="sap-icon://time-account" press="onPressToolbarButton" class="tmbButton">
			<customData>
				<core:CustomData key="buttonTarget" value="RouteClock"/>
			</customData>
		</Button>
		<Button icon="sap-icon://unfavorite" press="onPressToolbarButton" class="tmbButton">
			<customData>
				<core:CustomData key="buttonTarget" value="RouteStar"/>
			</customData>
		</Button>
	</Toolbar>
</core:FragmentDefinition>
